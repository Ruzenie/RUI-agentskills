{
  "version": "1.0.0",
  "structure_source": "internal-skills-architecture",
  "strategy": "shared-implementation-with-bilingual-entries",
  "locales": [
    {
      "id": "CN",
      "entry": "skills/variants/CN/SKILLS.md",
      "language": "zh-CN"
    },
    {
      "id": "EN",
      "entry": "skills/variants/EN/SKILLS.md",
      "language": "en-US"
    }
  ],
  "platform_templates": [
    {
      "platform": "codex",
      "entry_template_cn": "skills/platforms/codex/AGENTS.template.md",
      "entry_template_en": "skills/platforms/codex/AGENTS.template.en.md"
    },
    {
      "platform": "claude",
      "entry_template_cn": "skills/platforms/claude/CLAUDE.template.md",
      "entry_template_en": "skills/platforms/claude/CLAUDE.template.en.md"
    }
  ],
  "bundle_export": {
    "script": "skills/skill-structure-governor/scripts/export_skill_bundles.py",
    "outputs": [
      "RUI-codex-cn",
      "RUI-codex-en",
      "RUI-claude-cn",
      "RUI-claude-en"
    ]
  }
}
